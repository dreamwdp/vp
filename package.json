  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "prettier",
      "jest"
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "react/forbid-prop-types": 0,
      "prettier/prettier": [
        "error"
      ]
    },
    "env": {
      "browser": true,
      "jest/globals": true
    }
  },
  "jest": {
    "verbose": false,
    "clearMocks": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/client/__mocks__/fileMock.js",
      "\\.(css|sass)$": "<rootDir>/src/client/__mocks__/styleMock.js"
    },
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -10
      }
    }
  }
